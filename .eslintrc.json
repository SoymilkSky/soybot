{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "no-promise-executor-return": "off",
        "no-new": "off",
        "import/extensions": "off",
        "global-require": "off",
        "import/no-dynamic-require": "off",
        "no-unused-vars": "off"
    }
}
